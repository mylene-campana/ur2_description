<?xml version="1.0"?>
<robot name="cylinder_obstacle"> <!-- simple obstacle-->
  <link name="obstacle_base">
    <visual>
      <origin xyz="0.6 1.1 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.6 1.1 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </collision>
  </link>

  <joint name="j_cylinder2" type="fixed">
    <parent link="obstacle_base"/>
    <child link="l_cylinder2"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="l_cylinder2">
    <visual>
      <origin xyz="-0.3 -1.1 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.3 -1.1 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </collision>
  </link>

  <joint name="j_cylinder3" type="fixed">
    <parent link="obstacle_base"/>
    <child link="l_cylinder3"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="l_cylinder3">
    <visual>
      <origin xyz="0.42 -1.2 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.42 -1.2 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.2"/>
      </geometry>
    </collision>
  </link>
</robot>
